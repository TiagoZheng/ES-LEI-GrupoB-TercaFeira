<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Web App</display-name>
  
  <servlet>
      <servlet-name>HorarioServlet</servlet-name>
      <servlet-class>servlets.HorarioServlet</servlet-class>
  </servlet>
  <servlet>
      <servlet-name>UploadServlet</servlet-name>
      <servlet-class>servlets.UploadServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>WebcalServlet</servlet-name> 
  	<servlet-class>servlets.WebcalServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DownloadServlet</servlet-name>
  	<display-name>DownloadServlet</display-name>
  	<description></description>
  	<servlet-class>servlets.DownloadServlet</servlet-class>
  </servlet>

  <servlet-mapping>
      <servlet-name>HorarioServlet</servlet-name>
      <url-pattern>/horario</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>UploadServlet</servlet-name>
      <url-pattern>/uploadfile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WebcalServlet</servlet-name>
  	<url-pattern>/webcalservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DownloadServlet</servlet-name>
  	<url-pattern>/downloadservlet</url-pattern>
  </servlet-mapping>
  
</web-app>
